@page
@using LibraryManagement.Localization
@using LibraryManagement.Web.Pages.Books
@using Microsoft.Extensions.Localization
@model LibraryManagement.Web.Pages.Books.IndexModel
@inject IStringLocalizer<LibraryManagementResource> L
@section scripts
{
    <abp-script src="/Pages/Books/js/Index.js" />
}
<abp-card>
    <abp-card-header>
        <h2>@L["Books"]</h2>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="BooksTable"></abp-table>
    </abp-card-body>
</abp-card>
